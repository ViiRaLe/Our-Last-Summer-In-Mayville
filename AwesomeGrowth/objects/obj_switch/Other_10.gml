/// @description Activation - Toggle 
interIsActive = !interIsActive;

if (interIsActive)
{
    sprite_index = interSpriteActivated;
}
else
{
    sprite_index = interSpriteDeactivated;
}

if(interTarget != 0)
{
	with(interTarget)
	{
		// target activation
		if(interIsActive)
		{
			event_user(1);
		}
		else
		{
			event_user(0);
		}
	}
}

